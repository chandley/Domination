<head>
  <link rel="stylesheet" href="/css/jquery-jvectormap-2.0.1.css" type="text/css" media="screen"/>
  <script type="text/javascript" src="https://code.jquery.com/jquery-2.1.3.min.js"></script>
  <script type="text/javascript" src="/js/jquery-jvectormap-2.0.1.min.js"></script>
  <script type="text/javascript" src="/js/jquery-jvectormap-europe-mill-en.js"></script>
</head>
<body>
  <div id="map" style="width: 100%; height: 100%"></div>
  <script>
    $(function(){
      $('#map').vectorMap({map: 'europe_mill_en'});
  });
  </script>





<script>
$(function(){
$('#map').vectorMap({map: 'europe_mill_en'});
 $('svg path[data-code="GB"]').attr('fill','red')
 });
 </script>

<script>
$(function(){
$('#map').vectorMap({map: 'europe_mill_en'});
 $('svg path[data-code="FR"]').attr('fill','blue')
 });
 </script>

 <script>
$(function(){
$('#map').vectorMap({map: 'europe_mill_en'});
 $('svg path[data-code="SE"]').attr('fill','blue')
 });
 </script>


</body>